{
  "name": "merc",
  "version": "0.0.0",
  "description": "A Mercurial wrapper the makes Mercurial faster",
  "author": "Alexander Dvinsky",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "20.0.3",
    "babel-plugin-idx": "^1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-inline-imports-commonjs": "1.2.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-watch": "2.0.6",
    "eslint": "3.15.0",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-merc-eslint": "file:eslint-plugin-merc-eslint",
    "eslint-plugin-prefer-object-spread": "1.1.0",
    "eslint-plugin-prettier": "2.1.2",
    "flow-bin": "0.48.0",
    "flow-typed": "^2.1.2",
    "idx": "^1.5.0",
    "jest": "20.0.4",
    "precommit-hook-eslint": "^3.0.0",
    "prettier": "1.3.1",
    "split": "1.0.0"
  },
  "scripts": {
    "debug": "node node_modules/babel-watch/babel-watch.js --debug 5858 src/cli.js",
    "lint": "node node_modules/.bin/eslint --max-warnings=0 .",
    "watch": "node --trace-deprecation node_modules/babel-watch/babel-watch.js src/cli.js",
    "test": "node node_modules/.bin/jest"
  },
  "bin": {
    "merc": "./src/cli.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/(fixtures|__mocks__)/",
      "/_(temp|mock|install|init).js$"
    ]
  },
  "private": true,
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "fb-watchman": "^2.0.0",
    "json-stringify-safe": "^5.0.1",
    "nuclide-commons": "^0.1.8",
    "rxjs": "5.3.1",
    "yargs": "^8.0.2"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
